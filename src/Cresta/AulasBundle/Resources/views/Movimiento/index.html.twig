{% extends '::base.html.twig' %}
{% block title %}Movimientos {% endblock %}
{% block body -%}
    <h1>Lista de Movimientos</h1>

    <table class="records_list tabla">
        <thead>
            <tr>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ fechaDeHoy }}</td>
                <td>
                <ul>
                        <a href="{{ path('aulas_movimiento_show', { 'id': entity.id }) }}"><img src="{{ asset('img/iconos/ver.png') }}" class="icono" alt="Ver" /></a>                        
                        
                        
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

