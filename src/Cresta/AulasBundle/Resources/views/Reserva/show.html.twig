{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reserva</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observaciones }}</td>
            </tr>
            <tr>
                <th>Fecharegistro</th>
                <td>{{ entity.fechaRegistro|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Horadesde</th>
                <td>{{ entity.horaDesde |date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>Horahasta</th>
                <td>{{ entity.horaHasta |date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechareserva</th>
                <td>{{ entity.fechaReserva|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('aulas_reserva') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('aulas_reserva_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
