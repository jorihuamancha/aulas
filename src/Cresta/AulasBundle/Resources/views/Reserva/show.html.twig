{% extends '::base.html.twig' %}
{% block title %}Ver reserva {% endblock %}
{% block body -%}
    <h1>Reserva</h1>

    <table class="record_properties">
        <tbody>
            {#<tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>#}
            <tr>
                <th>Fecha de Reserva</th>
                <td>{{ entity.fecha|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Hora desde</th>
                <td>{{ entity.horaDesde|date('H:i') }}</td>
            </tr>
            <tr>
                <th>Hora hasta</th>
                <td>{{ entity.horaHasta|date('H:i') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Usuario</th>
                <td>{{ entity.usuario }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ entity.observaciones }}</td>
            </tr>
            <tr>
                <th>Fecha de registro</th>
                <td>{{ entity.fechaRegistro|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Hora de registro</th>
                <td>{{ entity.horaRegistro|date('H:i') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reserva') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reserva_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
